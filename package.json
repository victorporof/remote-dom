{
  "name": "remote-dom",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "bundle:client": "webpack client/index.js --output-path dist/client --mode development",
    "bundle:content:client": "webpack content/client/index.js --output-path dist/content/client --mode development",
    "bundle:content:server": "webpack content/server/index.js --output-path dist/content/server --mode development",
    "bundle": "yarn bundle:client && yarn bundle:content:client && yarn bundle:content:server",
    "start": "yarn bundle && node server/index.js"
  },
  "dependencies": {
    "express": "^4.17.1",
    "is-reachable": "^4.0.0",
    "lodash": "^4.17.15",
    "morgan": "^1.10.0",
    "normalize-url": "^5.0.0",
    "optimal-select": "^4.0.1",
    "puppeteer": "^3.0.4",
    "puppeteer-firefox": "^0.5.1",
    "shortid": "^2.2.15",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
