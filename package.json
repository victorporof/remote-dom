{
  "name": "remote-dom",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "bundle:client": "webpack client/index.js --output-path dist/client --mode development",
    "bundle:content:client": "webpack content/client/index.js --output-path dist/content/client --mode development",
    "bundle": "yarn bundle:client && yarn bundle:content:client",
    "kill": "lsof -t -i:3000,3001 | xargs kill",
    "serve:ua": "node server/index.js",
    "serve:tests": "serve tests -l 3001",
    "serve": "yarn serve:ua & yarn serve:tests",
    "start": "yarn bundle && yarn serve"
  },
  "dependencies": {
    "express": "^4.17.1",
    "is-reachable": "^4.0.0",
    "lodash": "^4.17.15",
    "morgan": "^1.10.0",
    "normalize-url": "^5.0.0",
    "puppeteer": "^3.1.0",
    "shortid": "^2.2.15",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "serve": "^11.3.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
